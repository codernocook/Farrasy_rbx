{
    "manifest_version": 3,
    "name": "Farrasy",
    "version": "1.0.0",
    "description": "Make the Roblox website look better.",
    "content_scripts": [
        {
            "matches": ["http://*.roblox.com/*", "https://*.roblox.com/*"],
            "js": ["contentScript.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "https://farrasy.vercel.app/"
    ]
}
